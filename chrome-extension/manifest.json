{
    "manifest_version": 2,
    "name": "A Better Banner",
    "version": "0.0.0",
    "description": "The Banner Students Deserve",
    "browser_action": {
    },
    "background": {
        "scripts": ["dawgHouse.js"],
        "persistent": false
    },
    "author": "Eli Davis && Josh Hawkins",
    "content_scripts": [{
            "matches": ["https://my.msstate.edu/"],
            "css": ["material.css", "inject.css"],
            "js": ["inject.js"]
        },{
            "matches": ["https://my.msstate.edu/cp/home/displaylogin"],
            "js": ["forwardToSite.js"]
        }],
	"icons": { "16": "BB_16.png",
			   "48": "BB_48.png",
			   "128": "BB_128.png" },
    "permissions": ["tabs", "storage"],
    "version_name": "1.0.0",
    "version": "1.0.0"
}
