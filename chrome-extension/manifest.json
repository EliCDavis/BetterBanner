{
    "manifest_version": 2,
    "name": "Better Access",
    "short_name": "Better Access",
    "description": "An easier way to perform school related tasks",
    "background": {
        "scripts": ["dawgHouse.js"],
        "persistent": false
    },
    "author": "Eli Davis",
    "content_scripts": [{
            "matches": ["https://my.msstate.edu/"],
            "css": ["material.css", "inject.css"],
            "js": ["inject.js"]
        }, {
            "matches": ["https://my.msstate.edu/cp/home/displaylogin"],
            "js": ["forwardToSite.js"]
        }],
    "icons": {
        "16": "BB_16.png",
        "48": "BB_48.png",
        "128": "BB_128.png"
    },
    "permissions": ["tabs", "storage"],
    "version_name": "1.1.1",
    "version": "1.1.1"
}
